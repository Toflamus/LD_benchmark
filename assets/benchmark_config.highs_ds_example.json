{
  "version": 1,
  "run": {
    "test": "all",
    "algorithms": [
      "gdpopt.ldsda",
      "gdpopt.ldbd"
    ],
    "cstr_nt_list": [5, 10, 15, 20, 25, 30],
    "column_keys": null
  },
  "outputs": {
    "date": "today",
    "results_dir": null,
    "clear": false
  },
  "common": {
    "tee": false,
    "time_limit": 900,
    "direction_norm": "Linf",
    "mip_solver": "gurobi",
    "separation_solver": "scipy_highs_ds",
    "separation_solver_args": {
      "options": {
        "presolve": "on",
        "simplex_strategy": 1
      }
    },
    "subproblem_solver": "gams",
    "subproblem_solver_args": {
      "solver": "conopt",
      "add_options": [
        "option threads = 1;",
        "option reslim = 3600;",
        "option iterlim = 5000;",
        "$onecho > conopt.opt",
        "rtnwma 1e-06",
        "rtnopt 1e-06",
        "lstall 15",
        "rtvmin 1e-08",
        "lsmeth 2",
        "lsisol 1",
        "rtmaxv 1.0e10",
        "$offecho",
        "GAMS_MODEL.optfile = 1;"
      ]
    }
  }
}

