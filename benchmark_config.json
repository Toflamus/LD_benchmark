{
  "version": 1,
  "run": {
    "test": "all",
    "algorithms": [
      "gdpopt.ldsda",
      "gdpopt.ldbd"
    ],
    "cstr_nt_list": [5, 10, 15, 20, 25, 30],
    "column_keys": null
  },
  "outputs": {
    "date": "today",
    "results_dir": null,
    "clear": false
  },
  "common": {
    "tee": false,
    "time_limit": 900,
    "direction_norm": "Linf",
    "nlp_solver": "ipopt",
    "mip_solver": "gurobi",
    "separation_solver": "gurobi",
    "minlp_solver": "gams",
    "minlp_solver_args": {
      "solver": "ipopt",
      "add_options": [
            "option reslim = 3600;",
            "option optcr = 1e-8;",
            "option optca = 1e-8;",
            "option threads = 1;"
        ]
    }
  }
}
