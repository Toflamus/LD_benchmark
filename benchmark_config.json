{
  "version": 1,
  "run": {
    "test": "all",
    "algorithms": [
      "gdpopt.ldsda",
      "gdpopt.ldbd"
    ],
    "cstr_nt_list": [5, 10, 15, 20, 25, 30],
    "column_keys": null
  },
  "outputs": {
    "date": "today",
    "results_dir": null,
    "clear": false
  },
  "common": {
    "tee": false,
    "time_limit": 900,
    "direction_norm": "Linf",
    "nlp_solver": "ipopt",
    "mip_solver": "gurobi",
    "separation_solver": "gurobi",
    "minlp_solver": "ipopt",
    "minlp_solver_args": {
      "options": {
        "mu_strategy": "adaptive",     
        "tol": 1e-6,                  
        "acceptable_tol": 1e-5,        
        "acceptable_iter": 15,         
        "bound_push": 0.01,           
        "bound_frac": 0.01,            
        "mumps_pivtol": 1e-4,          
        "mumps_pivtolmax": 0.1,       
        "max_iter": 5000,              
        "diverging_iterates_tol": 1e20
      }
    }
  }
}
